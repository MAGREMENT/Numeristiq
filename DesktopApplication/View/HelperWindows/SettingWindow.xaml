<Window x:Class="DesktopApplication.View.HelperWindows.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:DesktopApplication.View.HelperWindows"
        mc:Ignorable="d"
        Title="SettingWindow" Height="450" Width="800">
    <Grid Background="{StaticResource Background2}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        
        <ScrollViewer Grid.Column="0" Width="200" Height="450"
                      Style="{StaticResource TransparentBarScrollView}">
            <StackPanel x:Name="TitlePanel" MinHeight="448"/>
        </ScrollViewer>
    </Grid>
</Window>
