<UserControl x:Class="View.Pages.Solver.UserControls.StrategyUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Width="180" Orientation="Horizontal" Margin="0, 2.5, 0, 2.5">
        <CheckBox Margin="0, 0, 10, 0" x:Name="StrategyUsage" Checked="OnChecked" Unchecked="OnUnChecked"></CheckBox>
        <TextBlock x:Name="StrategyName" TextWrapping="Wrap"></TextBlock>
    </StackPanel>
</UserControl>
