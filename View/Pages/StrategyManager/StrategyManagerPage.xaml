<pages:HandledPage x:Class="View.Pages.StrategyManager.StrategyManagerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:View.Pages.StrategyManager.UserControls"
      xmlns:pages="clr-namespace:View.Pages"
      mc:Ignorable="d"
      Title="StrategyManagerPage">
    <StackPanel Orientation="Vertical">
        <Menu Height="25">
            <MenuItem FontFamily="Segoe MDL2 Assets" Header="&#xE72B;" FontSize="20" Click="GoBack"/>
        </Menu>
        
        <StackPanel Orientation="Horizontal">
            <local:SearchUserControl x:Name="SearchBar" Margin="10"/>
            <local:OptionModifierUserControl x:Name="OptionModifier" Margin="10"/>
            <local:StrategyListUserControl x:Name="StrategyList" Margin="10"/>
        </StackPanel>
    </StackPanel>
</pages:HandledPage>
