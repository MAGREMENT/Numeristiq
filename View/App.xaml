<Application x:Class="View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="FirstWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
             <ResourceDictionary.MergedDictionaries>
                 <ResourceDictionary>
                     <Brush x:Key="Background1">#FFFFFF</Brush>
                     <Brush x:Key="Background2">#F9F9F9</Brush>
                     <Brush x:Key="Background3">#F0F0F0</Brush>
                     <Brush x:Key="Primary1">#FC880B</Brush>
                     <Brush x:Key="Primary2">#EE810C</Brush>
                     <Brush x:Key="Secondary1">#F52A0A</Brush>
                     <Brush x:Key="Secondary2">#E8280B</Brush>
                     <Brush x:Key="Accent">#F50EE2</Brush>
                     <Brush x:Key="Text">#000000</Brush>
                 </ResourceDictionary>
             </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>


