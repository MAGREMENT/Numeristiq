<UserControl x:Class="View.CustomNavigationUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Height="25" x:Name="Main" Orientation="Horizontal">
        <Button FontFamily="Segoe MDL2 Assets" Content="&#xE72B;" FontSize="20"
                Height="25" VerticalAlignment="Center" Width="25" Click="GoBack"></Button>
    </StackPanel>
</UserControl>
