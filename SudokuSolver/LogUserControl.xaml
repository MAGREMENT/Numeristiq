<UserControl x:Class="SudokuSolver.LogUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SudokuSolver"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Width="300" Orientation="Vertical">
        <TextBlock x:Name="Title" FontSize="22" FontWeight="Heavy" Width="300" TextAlignment="Center" TextWrapping="Wrap"/>
        <TextBlock x:Name="Text" FontSize="14" Width="300" TextWrapping="Wrap"/>
    </StackPanel>
</UserControl>
