<UserControl x:Class="SudokuSolver.StrategyListUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:inf="clr-namespace:SudokuSolver.Utils"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer Height="700" Width="200" Background="{x:Static inf:ColorManager.Background1}">
        <StackPanel MinHeight="700" Width="180" x:Name="List" Orientation="Vertical"/>
    </ScrollViewer>
</UserControl>
